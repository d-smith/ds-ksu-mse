package domain;


import java.util.HashSet;
import java.util.Set;

/**
 * Representation of the data associated with an ActivityDefinition.
 *
 */
public final class ActivityDefinition implements java.io.Serializable {


	private static final long serialVersionUID = -4918921723834626860L;
	private long activitydefinitionid;
	private long processdefinitionid;
	private Long swimlaneid;
	private String name;
	private String description;
	private Boolean isstart;
	private Boolean isend;
	private Set<ActivityProperty> activityProperties = new HashSet<ActivityProperty>(
			0);

	public ActivityDefinition() {
	}



	public long getActivitydefinitionid() {
		return this.activitydefinitionid;
	}

	public void setActivitydefinitionid(long activitydefinitionid) {
		this.activitydefinitionid = activitydefinitionid;
	}

	public long getProcessdefinitionid() {
		return this.processdefinitionid;
	}

	public void setProcessdefinitionid(long processdefinitionid) {
		this.processdefinitionid = processdefinitionid;
	}

	public Long getSwimlaneid() {
		return this.swimlaneid;
	}

	public void setSwimlaneid(Long swimlaneid) {
		this.swimlaneid = swimlaneid;
	}

	public String getName() {
		return this.name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getDescription() {
		return this.description;
	}

	public void setDescription(String description) {
		this.description = description;
	}

	public Boolean getIsstart() {
		return this.isstart;
	}

	public void setIsstart(Boolean isstart) {
		this.isstart = isstart;
	}

	public Boolean getIsend() {
		return this.isend;
	}

	public void setIsend(Boolean isend) {
		this.isend = isend;
	}

	public Set<ActivityProperty> getActivityProperties() {
		return this.activityProperties;
	}

	public void setActivityProperties(Set<ActivityProperty> activityProperties) {
		this.activityProperties = activityProperties;
	}

}
