package domain;


/**
 * Representation of the connections between activities in a process definition
 * graph.
 *
 */
public final class ConnectorDefinition implements java.io.Serializable {

	
	private static final long serialVersionUID = -2716308485064455191L;
	private long connectorid;
	private long sourceid;
	private long sinkid;
	private String expression;

	public ConnectorDefinition() {
	}


	public long getConnectorid() {
		return this.connectorid;
	}

	public void setConnectorid(long connectorid) {
		this.connectorid = connectorid;
	}


	public long getSourceid() {
		return this.sourceid;
	}

	public void setSourceid(long sourceid) {
		this.sourceid = sourceid;
	}

	public long getSinkid() {
		return this.sinkid;
	}

	public void setSinkid(long sinkid) {
		this.sinkid = sinkid;
	}

	public String getExpression() {
		return this.expression;
	}

	public void setExpression(String expression) {
		this.expression = expression;
	}

}
