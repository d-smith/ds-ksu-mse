package dao;

import domain.ProcessDefinition;

/**
 * Data access interface for process definitions.
 *
 */
public interface ProcessDefinitionDAO {
	/**
	 * Retrieve a process definition by name.
	 * @param name Name of the process definition to retrieve.
	 * @return Process definition if found, otherwise null.
	 */
	ProcessDefinition retrieveProcessDefinition(String name);
}
